\begin_body

\begin_layout Title
Neural DE: an evolutionary method based on Differential Evolution suitable
 for neural network training
\end_layout

\begin_layout Title Citation
Neural DE: an evolutionary method based on Differential Evolution suitable
 for neural network training
\end_layout

\begin_layout Author
Ioannis G.
 Tsoulos
\begin_inset Formula $^{1,*}$
\end_inset

, Vasileios Charilogis
\begin_inset Formula $^{2}$
\end_inset


\end_layout

